   <children>
      <TextField fx:id="txtUsername" layoutX="17.0" layoutY="10.0" prefHeight="34.0" prefWidth="275.0" promptText="username or email" />
      <PasswordField fx:id="txtPassword" layoutX="17.0" layoutY="71.0" prefHeight="34.0" prefWidth="275.0" promptText="password" />
      <Button fx:id="click" layoutX="155.0" layoutY="135.0" mnemonicParsing="false" onAction="#click" prefHeight="34.0" prefWidth="137.0"  text="Sign in" />
   </children>
</AnchorPane>
